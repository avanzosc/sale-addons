<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="view_blanket_order_form" model="ir.ui.view">
        <field name="model">sale.blanket.order</field>
        <field name="inherit_id" ref="sale_blanket_order.view_blanket_order_form" />
        <field name="arch" type="xml">
            <field name="partner_id" position="after">
                <field name="description" />
            </field>
        </field>
    </record>

    <record id="view_blanket_order_tree" model="ir.ui.view">
        <field name="model">sale.blanket.order</field>
        <field name="inherit_id" ref="sale_blanket_order.view_blanket_order_tree" />
        <field name="arch" type="xml">
            <field name="name" position="after">
                <field name="description" optional="show" />
            </field>
        </field>
    </record>

    <record id="view_blanket_order_search" model="ir.ui.view">
        <field name="model">sale.blanket.order</field>
        <field name="inherit_id" ref="sale_blanket_order.view_blanket_order_search" />
        <field name="arch" type="xml">
            <field name="name" position="after">
                <field name="description" />
            </field>
        </field>
    </record>
</odoo>
