<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="view_stock_picking_form" model="ir.ui.view">
        <field name="name">stock.picking.form</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.view_picking_form" />
        <field name="arch" type="xml">
            <xpath expr="//notebook" position="inside">
                <page string="Delivery Information">
                    <group string="Delivery Information">
                        <group>
                            <field name="delivery_timetable" />
                            <field name="sale_phone" />
                            <field name="delivery_note" />
                            <field name="install_address_id" />
                            <field name="install_date" />
                        </group>
                        <group>
                            <field name="install_type_id" />
                            <field name="technical_contact_id" />
                            <field name="training" />
                            <field name="installation_done_by_id" />
                            <field name="structure_type_id" />
                            <field name="initial_order_id" />
                        </group>
                    </group>

                </page>
            </xpath>
        </field>
    </record>
</odoo>
